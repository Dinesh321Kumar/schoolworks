mixin header
    #header.header
        .container-xxl.container.d-flex.align-items-md-end.justify-content-center.justify-content-md-between
            .brand
                a(href="index.html")
                    img(src="images/logo.svg", alt="", height="40", width="250")

            include navigation.pug
            +navigation()

            #navCall.callnav.d-lg-none
                -for (i=0; i<3; i++)
                    span.line